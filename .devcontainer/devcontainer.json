{
    "image": "mcr.microsoft.com/devcontainers/rust:latest",
    "customizations": {
      "vscode": {
        "settings": {
          "lldb.executable": "/usr/bin/lldb",
          "files.watcherExclude": {
            "**/target/**": true
          },
          "rust-analyzer.checkOnSave.command": "clippy"
        },
        "extensions": ["rust-lang.rust-analyzer", "serayuzgur.crates"]
      }
    }
  }
  